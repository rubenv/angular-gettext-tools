{
  "env": {
    "mocha": true,
    "node": true
  },
  "rules": {
    "curly": 2,
    "eqeqeq": 2,
    "keyword-spacing": [
      2,
      {}
    ],
    "wrap-iife": [2, "any"],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "new-cap": 2,
    "no-caller": 2,
    "dot-notation": 0,
    "no-undef": 2,
    "no-eq-null": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "one-var": [
      2,
      "never"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "no-underscore-dangle": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-infix-ops": 2,
    "no-trailing-spaces": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "consistent-this": [
      2,
      "self"
    ],
    "dot-notation": 2,
    "yoda": [
      2,
      "never"
    ]
  }
}
